mkdir -p ./legacy

cd ./legacy

#Clone Downloads
git clone git@github.com:EnviroMine/Downloads.git ./

cp "../EnviroMine-${ver_num}.jar" "./EnviroMine-${ver_num}.jar"
cp "../EnviroMine-deobf-${ver_num}.jar" "./EnviroMine-deobf-${ver_num}.jar"

#Commit the new file
git add .
git commit -m "Archived version ${ver_num}"
git push -u origin master

cd ../